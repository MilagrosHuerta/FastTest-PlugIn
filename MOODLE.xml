<!-- question: 13  -->
  <question type="multichoice">
    <name>
      <text>T01 - 13 MATEMÁTICAS</text>
    </name>
    <questiontext format="html">
      <text><![CDATA[<p>¿Cuál es la _<strong>derivada</strong> de la siguiente función? $$\frac{\sin(x)}{x-1}$$</p>]]></text>
    </questiontext>
    <generalfeedback format="html">
      <text><![CDATA[<p>Tomo nota de tus respuestas</p>]]></text>
    </generalfeedback>
    <defaultgrade>1</defaultgrade>
    <penalty>0.333333333333333</penalty>
    <hidden>0</hidden>
    <idnumber></idnumber>
    <single>true</single>
    <shuffleanswers>true</shuffleanswers>
    <answernumbering>abc</answernumbering>
    <correctfeedback format="html">
      <text><![CDATA[<p>Respuesta correcta.</p>]]></text>
    </correctfeedback>
    <partiallycorrectfeedback format="html">
      <text><![CDATA[<p>Respuesta parcialmente correcta.</p>]]></text>
    </partiallycorrectfeedback>
    <incorrectfeedback format="html">
      <text><![CDATA[<p>Respuesta incorrecta.</p>]]></text>
    </incorrectfeedback>
    <shownumcorrect/>
    <answer fraction="100" format="html">
      <text><![CDATA[<p>$$\frac{\cos(x)}{x-1}-\frac{\sin(x)}{(x-1)^2}$$</p>]]></text>
      <feedback format="html">
      <text><![CDATA[<p></p>]]></text>
      </feedback>
    </answer>
    <answer fraction="" format="html">
      <text><![CDATA[<p>$$\frac{\cos^2(x)}{x-1}$$</p>]]></text>
      <feedback format="html">
      <text><![CDATA[<p></p>]]></text>
      </feedback>
    </answer>
    <answer fraction="" format="html">
      <text><![CDATA[<p>$$-\frac{\cos^2(x)}{x-1}$$</p>]]></text>
      <feedback format="html">
      <text><![CDATA[<p></p>]]></text>
      </feedback>
    </answer>
</question>
